@import 'nib'

@keyframes loader {
	from {background-position: 0,0}
	to {background-position: -768px,0}
}

body:before
	content ''
	display block
	position absolute	
	top: 50%;	
	left 50%
	transform: translate(-50%,-50%)
	width 64px
	height 64px
	background-image url("../img/712.GIF")
	animation loader 1s steps(12) infinite
